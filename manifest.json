{
  "manifest_version": 2,
  "name": "Korail Macro",
  "description": "KORAIL 자동 예약 매크로",
  "version": "1.0",
  "permissions": [
    "http://www.korail.com/"
  ],
  "content_scripts": [ {
    "all_frames": true,
    "js": ["jquery-1.11.0.min.js", "content.js"],
    "matches": ["http://www.korail.com/*"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [
    "inject.js", "macro.gif", "macrostop.gif"
  ]
}